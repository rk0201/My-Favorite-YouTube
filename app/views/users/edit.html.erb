  <%= form_for @user do |f| %>
  <div class="field">
    <%= form.label :login_id %>
    <%= form.text_area :login_id %>
  </div>
  
  <div class="field">
    <%= form.label :name %>
    <%= form.text_area :name %>
  </div>

  <div class="field">
    <%= form.label :icon %>
    <%= form.text_area :icon %>
  </div>

  <div class="field">
    <%= form.label :profile %>
    <%= form.text_area :profile %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

