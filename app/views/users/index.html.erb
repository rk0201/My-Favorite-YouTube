<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Name</th>
      <th>Icon</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.login_id %></td>
        <td><%= link_to  user.name,user_path(user) %></td>
        <td><%= user.icon %></td>
        <td><%= user.profile %></td>
      </tr>
    <% end %>
  </tbody>
</table>
